Cell[BoxData[
 RowBox[{
  RowBox[{"Bisec", "[", 
   RowBox[{
   "f_", ",", "a0_", ",", "b0_", ",", "xtol_", ",", "ytol_", ",", "pmax_"}], 
   "]"}], ":=", "\n", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"c", ",", "fa", ",", "fb", ",", "fc", ",", "i"}], "}"}], ",", 
    "\n", 
    RowBox[{
     RowBox[{"fa", "=", 
      RowBox[{"f", "[", "a0", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"fb", "=", 
      RowBox[{"f", "[", "b0", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"c", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a0", "+", "b0"}], ")"}], "/", "2.00"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"fc", "=", 
      RowBox[{"f", "[", "c", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"i", "\[LessEqual]", "pmax"}], ")"}], "&&", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"c", "-", "a0"}], "]"}], "\[GreaterEqual]", "xtol"}], 
           ")"}], " ", "&&", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Abs", "[", "fc", "]"}], "\[GreaterEqual]", "ytol"}], 
           ")"}]}], ",", "\[IndentingNewLine]", 
         RowBox[{"If", "[", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Sign", "[", "fb", "]"}], "*", 
              RowBox[{"Sign", "[", "fc", "]"}]}], ">", "0"}], ")"}], ",", 
           RowBox[{
            RowBox[{"b0", "=", "c"}], ";", 
            RowBox[{"fb", "=", "fc"}], ";"}]}], "]"}], ",", 
         RowBox[{"i", "++"}]}], "]"}], "\[IndentingNewLine]", "a0"}], "=", 
      "c"}], ";", "\[IndentingNewLine]", 
     RowBox[{"fa", "=", "fc"}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", 
      RowBox[{"i", "+", "1"}]}], ";", 
     RowBox[{"c", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a0", "+", "b0"}], ")"}], "/", "2.0"}]}], ";", 
